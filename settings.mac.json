{
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#000000",
  "vim.easymotionMarkerForegroundColorOneChar": "#ff0000",
  "vim.easymotionMarkerForegroundColorTwoChar": "#ffff00",
  "vim.easymotionMarkerFontFamily": "Arial",
  "vim.easymotionMarkerHeight": 12,
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerYOffset": 4,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-;>"
      ],
      "after": [
        "<Esc>",
        "o"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    {
      "before": [
        ","
      ],
      "after": [
        "leader",
        "leader",
        "leader",
        "b",
        "d",
        "w"
      ]
    },
    {
      "before": [
        "leader",
        "w"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "i",
        "w",
        "P"
      ]
    },
    {
      "before": [
        "leader",
        "t"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "i",
        "t",
        "P"
      ]
    },
    {
      "before": [
        "leader",
        "l"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "d",
        "P"
      ]
    },
    {
      "before": [
        ";"
      ],
      "after": [
        ":"
      ]
    },
    {
      "before": [
        "leader",
        "n"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.rename",
          "args": []
        }
      ]
    },
    {
      "before": [
        "leader",
        "'"
      ],
      "after": [
        "y",
        "s",
        "i",
        "w",
        "'"
      ]
    },
    {
      "before": [
        "leader",
        "\""
      ],
      "after": [
        "y",
        "s",
        "i",
        "w",
        "\""
      ]
    }
  ],
  "vim.leader": "<space>",
  "editor.lineNumbers": "relative",
  "window.titleBarStyle": "custom",
  "terminal.integrated.rendererType": "dom",
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "explorer.confirmDragAndDrop": false,
  "editor.tabSize": 2,
  "javascript.validate.enable": false,
  "git.autofetch": true,
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 10,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "eslint.format.enable": true,
  "workbench.colorTheme": "One Dark Pro",
  "editor.fontSize": 14,
  "editor.cursorBlinking": "solid",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ffffff",
    "editorCursor.background": "#000000",
    "terminalCursor.foreground": "#ffffff",
    "terminalCursor.background": "#000000",
  },
  "mocha.nycPath": "/usr/local/bin/nyc",
  "explorer.compactFolders": false
}