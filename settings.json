{
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#000000",
  "vim.easymotionMarkerForegroundColorOneChar": "#ff0000",
  "vim.easymotionMarkerForegroundColorTwoChar": "#ffff00",
  "vim.easymotionMarkerFontFamily": "Arial",
  "vim.easymotionMarkerHeight": 12,
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerYOffset": 4,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [","],
      "after": ["<leader>", "<leader>", "<leader>", "b", "d", "w"]
    },
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["editor.action.rename"]
    }
  ],
  "vim.leader": "<space>",
  "editor.lineNumbers": "relative",
  "window.zoomLevel": 1.1,
  "window.titleBarStyle": "custom",
  "terminal.integrated.rendererType": "dom",
  "editor.minimap.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Nord",
  "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "explorer.confirmDragAndDrop": false
}
